PRODUCTS.NAME AS NAME,
PRODUCTS.CATEGORY,
CATEGORIES.NAME AS CATEGORYNAME,
STOCKCURRENT.UNITS,
COALESCE(STOCKCURRENT.STOCKSECURITY, 0) AS STOCKSECURITY,
COALESCE(STOCKCURRENT.STOCKMAXIMUM, 0) AS STOCKMAXIMUM
